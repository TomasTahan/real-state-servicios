# .env.example
# Copia este archivo a .env y completa con tus credenciales

# Browser-Use API Key (obtener en https://browser-use.com)
BROWSER_USE_API_KEY=tu-browser-use-api-key

# Supabase Configuration (obtener en tu proyecto de Supabase)
SUPABASE_URL=https://tu-proyecto.supabase.co
SUPABASE_KEY=tu-supabase-anon-key

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Browser-Use Settings
BROWSER_USE_CLOUD=true
MAX_FAILURES=3
STEP_TIMEOUT=30
MAX_ACTIONS_PER_STEP=5
